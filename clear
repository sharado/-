[32m+[m[32m.background-size (@string: contain) {[m
[32m+[m[32m    -webkit-background-size: @string;[m
[32m+[m[32m    -moz-background-size: @string;[m
[32m+[m[32m    -o-background-size: @string;[m
[32m+[m[32m    background-size: @string;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// retina.less[m
[32m+[m[32m// A helper mixin for applying high-resolution background images (http://www.retinajs.com)[m
[32m+[m
[32m+[m[32m@highdpi: ~"(-webkit-min-device-pixel-ratio: 1.5), (min--moz-device-pixel-ratio: 1.5), (-o-min-device-pixel-ratio: 3/2), (min-resolution: 1.5dppx)";[m
[32m+[m
[32m+[m[32m.at2x(@path, @w: auto, @h: auto) {[m
[32m+[m[32m  background-image: url(@path);[m
[32m+[m[32m  @at2x_path: ~`@{path}.replace(/\.\w+$/, function(match) { return "@2x" + match; })`;[m
[32m+[m
[32m+[m[32m  @media @highdpi {[m
[32m+[m[32m    background-image: url("@{at2x_path}");[m
[32m+[m[32m    background-size: @w @h;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m

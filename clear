[32m+[m[41m [m	[32mbackground-color: @startColor;[m
[32m+[m	[32mbackground-image: -webkit-gradient(linear, left top, right top, from(@startColor), to(@endColor));[m
[32m+[m	[32mbackground-image: -webkit-linear-gradient(left, @startColor, @endColor);[m
[32m+[m	[32mbackground-image: -moz-linear-gradient(left, @startColor, @endColor);[m
[32m+[m	[32mbackground-image: -ms-linear-gradient(left, @startColor, @endColor);[m
[32m+[m	[32mbackground-image: -o-linear-gradient(left, @startColor, @endColor);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.animation (@name, @duration: 300ms, @delay: 0, @ease: ease) {[m

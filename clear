[32m+[m[32m.text-shadow (@string: 0 1px 3px rgba(0, 0, 0, 0.25)) {[m
[32m+[m	[32mtext-shadow: @string;[m
[32m+[m[32m}[m
[32m+[m[32m.box-shadow (@string) {[m
[32m+[m	[32m-webkit-box-shadow: @string;[m
[32m+[m	[32m-moz-box-shadow:    @string;[m
[32m+[m	[32mbox-shadow:         @string;[m
[32m+[m[32m}[m
[32m+[m[32m.drop-shadow (@x: 0, @y: 1px, @blur: 2px, @spread: 0, @alpha: 0.25) {[m
[32m+[m	[32m-webkit-box-shadow:	@x @y @blur @spread rgba(0, 0, 0, @alpha);[m
[32m+[m	[32m-moz-box-shadow:	@x @y @blur @spread rgba(0, 0, 0, @alpha);[m
[32m+[m	[32mbox-shadow:		@x @y @blur @spread rgba(0, 0, 0, @alpha);[m
[32m+[m[32m}[m
[32m+[m[32m.inner-shadow (@x: 0, @y: 1px, @blur: 2px, @spread: 0, @alpha: 0.25) {[m
[32m+[m	[32m-webkit-box-shadow: inset @x @y @blur @spread rgba(0, 0, 0, @alpha);[m
[32m+[m	[32m-moz-box-shadow:    inset @x @y @blur @spread rgba(0, 0, 0, @alpha);[m
[32m+[m	[32mbox-shadow:         inset @x @y @blur @spread rgba(0, 0, 0, @alpha);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.box-sizing (@type: border-box) {[m
[32m+[m	[32m-webkit-box-sizing: @type;[m
[32m+[m	[32m-moz-box-sizing:    @type;[m
[32m+[m	[32mbox-sizing:         @type;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.border-radius (@radius: 5px) {[m
[32m+[m	[32m-webkit-border-radius: @radius;[m
[32m+[m	[32m-moz-border-radius:    @radius;[m
[32m+[m	[32m-ms-border-radius:     @radius;[m
[32m+[m[32m    -o-border-radius:      @radius;[m
[32m+[m	[32mborder-radius:         @radius;[m
[32m+[m
[32m+[m	[32m-moz-background-clip:    padding;[m
[32m+[m	[32m-webkit-background-clip: padding-box;[m
[32m+[m	[32mbackground-clip:         padding-box;[m
[32m+[m[32m}[m
[32m+[m[32m.border-radiuses (@topright: 0, @bottomright: 0, @bottomleft: 0, @topleft: 0) {[m
[32m+[m	[32m-webkit-border-top-right-radius:    @topright;[m
[32m+[m	[32m-webkit-border-bottom-right-radius: @bottomright;[m
[32m+[m	[32m-webkit-border-bottom-left-radius:  @bottomleft;[m
[32m+[m	[32m-webkit-border-top-left-radius:     @topleft;[m
[32m+[m
[32m+[m	[32m-moz-border-radius-topright:        @topright;[m
[32m+[m	[32m-moz-border-radius-bottomright:     @bottomright;[m
[32m+[m	[32m-moz-border-radius-bottomleft:      @bottomleft;[m
[32m+[m	[32m-moz-border-radius-topleft:         @topleft;[m
[32m+[m
[32m+[m	[32mborder-top-right-radius:            @topright;[m
[32m+[m	[32mborder-bottom-right-radius:         @bottomright;[m
[32m+[m	[32mborder-bottom-left-radius:          @bottomleft;[m
[32m+[m	[32mborder-top-left-radius:             @topleft;[m
[32m+[m
[32m+[m	[32m-moz-background-clip:    padding;[m[41m [m
[32m+[m	[32m-webkit-background-clip: padding-box;[m[41m [m
[32m+[m	[32mbackground-clip:         padding-box;[m[41m [m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.opacity (@opacity: 0.5) {[m
[32m+[m	[32m-webkit-opacity: 	@opacity;[m
[32m+[m	[32m-moz-opacity: 		@opacity;[m
[32m+[m	[32mopacity: 		@opacity;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.gradient (@startColor: #eee, @endColor: white) {[m
[32m+[m	[32mbackground-color: @startColor;[m
[32m+[m	[32mbackground: -webkit-gradient(linear, left top, left bottom, from(@startColor), to(@endColor));[m
[32m+[m	[32mbackground: -webkit-linear-gradient(top, @startColor, @endColor);[m
[32m+[m	[32mbackground: -moz-linear-gradient(top, @startColor, @endColor);[m
[32m+[m	[32mbackground: -ms-linear-gradient(top, @startColor, @endColor);[m
[32m+[m	[32mbackground: -o-linear-gradient(top, @startColor, @endColor);[m
[32m+[m[32m}[m
